<script language="JavaScript" type="text/JavaScript" smarty>
function change_products(product_class){
  $("ul#products li").hide();
  $("ul#products li." + product_class).show();
  $("ul#product_menu li").removeClass('current');
  $("ul#product_menu li." + product_class).addClass('current');
}
